module vector

use crate::vector::vector::*

fn main():
	fibonacci(35)
	pass

fn math_expression(a: u64, b: u64, c: u64) -> u64:
	(a + b) * c

fn fibonacci(n: u64) -> u64:
	let ~first = 1
	let ~second = 1

	let ~counter = 1
	loop counter == 1 => counter == n:
		let summation = first + second
		first <=> second
		second <=> summation

		drop summation = second - first
		counter += 1

	drop n = second
	second
