fn 💯(💕: u64) -> u64 {
	when {
		💕 == 0 => 💕 == 0 -> 0,
		💕 == 1 => 💕 == 1 -> 1,
		true => true -> {
			let 🤛 = 💕 - 2;
			let 👈 = 💯(🤛);
			let 🤜 = 💕 - 1;
			let 👉 = 💯(🤜);
			let 😍 = 👈 + 👉;

			drop 👈 = 💯(🤛);
			drop 👉 = 💯(🤜);
			drop 🤛 = 💕 - 2;
			drop 🤜 = 💕 - 1;
			😍
		},
	}
}

fn main() {
	trace(💯(28));
}
